{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth": {
    "AdminGroup": "Admins",
    "ViewerGroup": "Viewers",
    "AllowCors": false,
    "AccessTokenCookie": "jwtAccessCookie",
    "RefreshTokenCookie": "jwtRefreshCookie",
    "TokenRefreshMinutes": 5,
    "Jwt": {
      "GroupsClaim": "cognito:groups",
      "Authority": "",
      "Audience": ""
    }
  },
  "CorsTrustedOrigins": [],
  "Spa": {
    "Active": false,
    "RootPath": "wwwroot",
    "PathsToReserve": ["login", "data"]
  },
  "Aws": {
    "Region": ""
  },
  "SignalRMultiplexer": {
    "Interval": "00:00:01:00",
    "RelayedHubs": [
      {
        "Url": "https://localhost:5001/job-hub",
        "Methods": [
          "PlayerStatusTrackerJob"
        ]
      },
      {
        "Url": "https://localhost:5002/job-hub",
        "Methods": [
          "PerformanceTrackerJob"
        ]
      },
      {
        "Url": "https://localhost:5003/job-hub",
        "Methods": [
          "PlayerCardTrackerJob",
          "CardPriceTrackerJob",
          "RosterUpdaterJob",
          "TrendReporterJob"
        ]
      }
    ]
  }
}
