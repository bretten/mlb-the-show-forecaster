{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth": {
    "AdminGroup": "Admins",
    "ViewerGroup": "Viewers",
    "AllowCors": false,
    "AccessTokenCookie": "jwtAccessCookie",
    "RefreshTokenCookie": "jwtRefreshCookie",
    "TokenRefreshMinutes": 5,
    "Jwt": {
      "GroupsClaim": "cognito:groups",
      "Authority": "",
      "Audience": ""
    }
  },
  "CorsTrustedOrigins": [],
  "Spa": {
    "Active": false,
    "RootPath": "wwwroot",
    "PathsToReserve": [
      "login",
      "jobs"
    ]
  },
  "Aws": {
    "Region": ""
  },
  "SignalRMultiplexer": {
    "Interval": "00:00:01:00"
  },
  "TargetApps": {
    "PlayerTracker": {
      "Scheme": "https",
      "Host": "localhost",
      "Port": 5001,
      "Methods": [
        "PlayerStatusTrackerJob"
      ]
    },
    "PerformanceTracker": {
      "Scheme": "https",
      "Host": "localhost",
      "Port": 5002,
      "Methods": [
        "PerformanceTrackerJob"
      ]
    },
    "MarketplaceWatcher": {
      "Scheme": "https",
      "Host": "localhost",
      "Port": 5003,
      "Methods": [
        "PlayerCardTrackerJob",
        "CardPriceTrackerJob",
        "RosterUpdaterJob",
        "TrendReporterJob"
      ]
    }
  }
}
